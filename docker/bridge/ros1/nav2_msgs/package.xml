<package>
  <!--
      This pkg is a subset of nav2_msgs to expose action to ROS1 via bridge
      https://github.com/ros-planning/navigation2/tree/main/nav2_msgs
  -->
  <name>nav2_msgs</name>
  <version>1.0.0</version>
  <description>Messages used for ROS1 bridge to Navigation2.</description>
  <maintainer email="daisukes@cmu.edu">Daisuke Sato</maintainer>

  <license>Apache-2.0</license>

  <buildtool_depend>catkin</buildtool_depend>
  
  <build_depend>actionlib_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>message_generation</build_depend>

  <run_depend>actionlib_msgs</run_depend>
  <run_depend>geometry_msgs</run_depend>
  <run_depend>message_runtime</run_depend>
</package>
